//http://momijiame.tumblr.com/post/121025309366/blockdiag-シリーズで色々な作図をしてみる
actdiag {
  start -> write -> processing -> image -> end

  lane user {
    start [shape = beginpoint]
    write [label = "設定を書く"]
    image [label = "画像を取得する"]
    end [shape = endpoint]
  }

  lane actdiag {
    processing [label = "画像に変換する"]
  }
}